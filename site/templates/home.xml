<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../units/schema/language.dtd">
<document>
	<cms:var name="_DOCTYPE"/>
	<html lang="$language" cms:eval="lang">
		<head>
			<title><cms:text constant="site_title"/></title>
			<cms:collection include="page_control"/>
			<cms:module name="head_tag" action="print_tag"/>
		</head>

		<body class="$language_rtl ? 'rtl' : 'ltr'" cms:eval="class">
			<header>
				<div class="inner_wrap">
					<div class="logo">
						<span></span>
					</div>
					<nav>
						<a href="#"><cms:text constant="home"/></a>
						<a href="#"><cms:text constant="about"/></a>
						<a href="#"><cms:text constant="moving"/></a>
						<a href="#"><cms:text constant="international"/></a>
						<a href="#"><cms:text constant="estimate"/></a>
						<a href="#"><cms:text constant="reviews"/></a>
						<a href="#"><cms:text constant="contact"/></a>
					</nav>
				</div>
			</header>
			<!-- Top Section image slides -->
			<div id="slides">
				<div class="slide"></div>
				<div class="slide"></div>
				<div class="slide"></div>
				<div class="inner_wrap">
					<div class="inner_form">
						<h2><cms:text constant="form_title"/></h2>
						<div class="form_menu">
							<a
								href="javascript:void(0);"
								class="arrow next"
								>
								<cms:text constant="form_local"/>
							</a>
							<a
								href="javascript:void(0);"
								class="arrow previous"
								>
								<cms:text constant="form_inter"/>
							</a>
						</div>
						<div class="form_container">
								<cms:module
									name="contact_form"
									action="show"
									text_id="contact_local"
									/>
								<cms:module
									name="contact_form"
									action="show"
									text_id="contact_international"
									/>

						</div>
					</div>
				</div>
			</div>
				</body>
	</html>
</document>
